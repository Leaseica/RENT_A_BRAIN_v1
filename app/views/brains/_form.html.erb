<%= simple_form_for brain,
  data: {
      insert_in_list_target: "form",
      action: "submit->insert-in-list#send"
    } do |f| %>
  <%= f.input :title %>
  <%= f.input :address %>
  <%= f.input :photo, as: :file %>
  <%= f.input :description %>
  <%= f.input :wiki_page %>
  <%= f.input :specialty %>
  <%= f.input :price %>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
